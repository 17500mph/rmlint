#!/usr/bin/env python
# encoding: utf-8

Import('env')
Import('create_uninstall_target')


import os
import time
import subprocess


if 'install' in COMMAND_LINE_TARGETS:
    py_install = env.Command(
        'always.install',
        ['setup.py'],
        'cd gui && python3 setup.py install'
    )
    env.Alias('install', py_install)


# No uninstall target yet.
